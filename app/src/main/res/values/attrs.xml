    <attr name="title_text_color" format="color"/>
    <!--主界面头部颜色-->
    <attr name="main_title_background" format="reference|color"/>
    <!--登录按钮-->
    <attr name="login_button" format="reference"/>
    <!--保存记录按钮-->
    <attr name="add_bus_button" format="reference"/>
